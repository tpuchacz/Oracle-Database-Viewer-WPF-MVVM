<Window x:Class="MVVMToolkit.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVMToolkit"
        FontSize="24" MinHeight="300" MinWidth="300"
        mc:Ignorable="d" d:DataContext="{d:DesignInstance Type=local:MainWindowViewModel}" WindowStartupLocation="CenterScreen"
        Title="Oracle Database Viewer" Height="450" Width="800">
    <Grid>
        <ContentControl Content="{Binding SelectedViewModel}">
        </ContentControl>
    </Grid>
</Window>
